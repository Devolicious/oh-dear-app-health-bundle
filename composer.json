{
    "name": "devolicious/oh-dear-app-health-bundle",
    "description": "A simple Symfony bundle to integrate OhDear Application Health monitoring",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.1",
        "ohdearapp/health-check-results": "^1.0",
        "symfony/framework-bundle": "^5.4 | ^6.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devolicious\\OhDearAppHealthBundle\\": ""
        }
    },
    "authors": [
        {
            "name": "Ruud Denivel",
            "email": "ruud@devolution.be"
        }
    ],
    "require-dev": {
        "phpro/grumphp-shim": "^2.2",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.38",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^9.0",
        "phpunit/php-code-coverage": "^9.0",
        "symfony/phpunit-bridge": "^6.3",
        "doctrine/orm": "^2.16"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
